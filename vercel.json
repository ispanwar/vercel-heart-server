{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ],
  "env": {
    "MODEL_PATH": "/model.pkl"
  },
  "functions": {
    "app.py": {
      "memory": 512,
      "maxDuration": 30
    }
  }
}
